{
  "buildCommand": "rm -rf node_modules/.prisma && DATABASE_URL='postgresql://dummy:dummy@dummy:5432/dummy' npm run vercel-build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["cdg1"],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "includeFiles": "node_modules/.prisma/client/**",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}

