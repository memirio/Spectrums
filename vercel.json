{
  "buildCommand": "DATABASE_URL=\"postgresql://dummy:dummy@dummy:5432/dummy\" prisma generate && DATABASE_URL=\"postgresql://dummy:dummy@dummy:5432/dummy\" npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "includeFiles": "node_modules/.prisma/client/**",
      "maxDuration": 30
    },
    ".next/server/**/*.js": {
      "includeFiles": "node_modules/.prisma/client/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}

